{
    "name": "thecvlb/svc-sdk-php",
    "description": "SVC SDK for PHP - Use LifeMD Services in your PHP project",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.4|^8.1",
        "laminas/laminas-diactoros": "^2.17",
        "psr/http-factory": "^1.0",
        "psr/http-client": "^1.0",
        "php-http/client-common": "^2.6",
        "php-http/curl-client": "^2.3",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.5",
        "php-http/mock-client": "^1.5",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "CVLB\\Svc\\Api\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
